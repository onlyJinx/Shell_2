{
    "log": {
        "access": "/etc/v2ray/access.log",
        "error": "/etc/v2ray/error.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": XTLS_PORT,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "XTLS_UUID",
                        "flow": "xtls-rprx-direct",
                        "level": 0,
                        "email": "love@example.com"
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": DESP_PORT,
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                },
                "security": "xtls",
                "xtlsSettings": {
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/ssl/SSL_XRAY_CER.cer",
                            "keyFile": "/ssl/SSL_XRAY_KEY.key"
                        }
                    ]
                }
            }
        },
        {
            "port": VLESS_GRPC_PORT,
            "listen": "127.0.0.1",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "VLESS_GRPC_UUID"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "grpc",
                "grpcSettings": {
                    "serviceName": "VLESS_GRPC_NAME"
                }
            }
        },
        {
            "port": WS_PORT,
            "listen": "127.0.0.1",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "WS_UUID",
                        "level": 0,
                        "email": "love@example.com"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "path": "/WS_PATH"
                }
            }
        },
        {
            "port": DESP_PORT,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "V2RAY_TROJAN_PASSWORD",
                        "level": 0,
                        "email": "love@example.com"
                    }
                ],
                "fallbacks": [
                    {
                        "dest": 5555
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                }
            }
        },
        {
            "port": TROJAN_GRPC_PORT,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password":"TROJAN_GRPC_PASSWD",
                        "email": "love@v2fly.org"
                    }
                ]
            },
            "streamSettings": {
                "network": "grpc",
                "grpcSettings": {
                    "serviceName": "TROJAN_GRPC_SERVICE_NAME"
                }
            }
        }
    ],
    "outbounds": [
        {
            "tag": "DIRECT",
            "protocol": "freedom"
        },
        {
            "tag": "BLOCKD_CN",
            "protocol": "Blackhole"
        },
        {
            "tag": "BLOCKD_ADS",
            "protocol": "Blackhole"
        }
    ],
    "routing": {
        "domainStrategy": "IPOnDemand",
        "rules": [
            {
                "domainMatcher": "mph",
                "type": "field",
                "domains": [
                    "geosite:cn"
                ],
                "network": "tcp",
                "outboundTag": "BLOCKD_CN"
            },
            {
                "domainMatcher": "mph",
                "type": "field",
                "domains": [
                    "geosite:category-ads-all"
                ],
                "network": "tcp",
                "outboundTag": "BLOCKD_ADS"
            },
            {
                "domainMatcher": "mph",
                "type": "field",
                "ip": [
                    "127.0.0.1/16",
                    "192.168.0.0/16",
                    "geoip:cn"
                ],
                "outboundTag": "DIRECT"
            }
        ]
    }
}